    Metodo printMe()
        Escribir "\nPLANETA " + getNombrePlaneta()
        Escribir "--------------"
        Escribir "GUERRERO: " + getTipoGuerrero() + "\nDINERO PRODUCIDO: " + "Entre 50-100 Galactus" 
        + "\nCANTIDAD GUERREROS: " + "Entre 15-25 Guerreros Mole" + "\n"

